{
    "name": "@vue-layout/hyperscript",
    "version": "1.0.0",
    "description": "A package containing hyperscript components for usage in render functions.",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        }
    },
    "main": "dist/index.cjs",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "files": [
        "dist/*"
    ],
    "keywords": [
        "vue",
        "layout",
        "hyperscript",
        "components",
        "builder",
        "typescript",
        "template"
    ],
    "author": {
        "name": "Peter Placzek",
        "email": "contact@tada5hi.net",
        "url": "https://tada5hi.net"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/tada5hi/vue-layout.git",
        "directory": "packages/hyperscript"
    },
    "scripts": {
        "prebuild": "rimraf ./dist",
        "build:types": "tsc --emitDeclarationOnly -p tsconfig.build.json",
        "build:js": "rollup -c",
        "build": "cross-env NODE_ENV=production npm run build:js && npm run build:types",
        "lint": "eslint --ext .ts ./src/",
        "lint:fix": "npm run lint -- --fix"
    },
    "devDependencies": {
        "@types/lodash.template": "^4.5.1",
        "@vue-layout/core": "^1.0.0",
        "cross-env": "^7.0.3",
        "vue": "^3.2.45"
    },
    "peerDependencies": {
        "@vue-layout/core": "^1.1.0",
        "vue": "^3.x"
    },
    "engines": {
        "node": ">=16"
    },
    "dependencies": {
        "lodash.template": "^4.5.0",
        "smob": "^1.0.0"
    },
    "publishConfig": {
        "access": "public"
    }
}
