{
    "name": "@vue-layout/hyperscript",
    "version": "0.1.1",
    "description": "A package containing hyperscript components for usage in render functions.",
    "main": "dist/index.cjs.js",
    "browser": "dist/index.browser.js",
    "module": "dist/index.esm.js",
    "unpkg": "dist/index.iife.js",
    "types": "dist/types/index.d.ts",
    "files": [
        "dist/*"
    ],
    "keywords": [
        "vue",
        "layout",
        "hyperscript",
        "components",
        "builder",
        "typescript",
        "template"
    ],
    "author": {
        "name": "Peter Placzek",
        "email": "contact@tada5hi.net",
        "url": "https://tada5hi.net"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/tada5hi/vue-layout.git",
        "directory": "packages/hyperscript"
    },
    "scripts": {
        "prebuild": "rimraf ./dist",
        "build:types": "tsc --emitDeclarationOnly -p tsconfig.build.json",
        "build:js": "rollup -c",
        "build": "cross-env NODE_ENV=production npm run build:js && npm run build:types",
        "lint": "eslint --ext .ts ./src/",
        "lint:fix": "npm run lint -- --fix"
    },
    "devDependencies": {
        "@babel/core": "^7.19.3",
        "@babel/preset-env": "^7.19.4",
        "@babel/preset-typescript": "^7.18.6",
        "@rollup/plugin-alias": "^4.0.0",
        "@rollup/plugin-babel": "^6.0.0",
        "@rollup/plugin-commonjs": "^23.0.0",
        "@rollup/plugin-node-resolve": "^15.0.0",
        "@rollup/plugin-replace": "^5.0.0",
        "@vue-layout/core": "^0.1.0",
        "cross-env": "^7.0.3",
        "rollup": "^2.79.0",
        "rollup-plugin-includepaths": "^0.2.4",
        "rollup-plugin-postcss": "^4.0.2",
        "rollup-plugin-styles": "^4.0.0",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-vue": "^6.0.0-beta.11",
        "vue": "^3.2.40"
    },
    "peerDependencies": {
        "@vue-layout/core": "^0.x",
        "vue": "^3.x"
    },
    "engines": {
        "node": ">=16"
    },
    "dependencies": {
        "regenerator-runtime": "^0.13.9",
        "smob": "^0.0.3"
    },
    "publishConfig": {
        "access": "public"
    }
}
