{
    "name": "@vue-layout/hyperscript",
    "version": "0.1.7",
    "description": "A package containing hyperscript components for usage in render functions.",
    "main": "dist/index.cjs.js",
    "browser": "dist/index.browser.js",
    "module": "dist/index.esm.js",
    "unpkg": "dist/index.iife.js",
    "types": "dist/types/index.d.ts",
    "files": [
        "dist/*"
    ],
    "keywords": [
        "vue",
        "layout",
        "hyperscript",
        "components",
        "builder",
        "typescript",
        "template"
    ],
    "author": {
        "name": "Peter Placzek",
        "email": "contact@tada5hi.net",
        "url": "https://tada5hi.net"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/tada5hi/vue-layout.git",
        "directory": "packages/hyperscript"
    },
    "scripts": {
        "prebuild": "rimraf ./dist",
        "build:types": "tsc --emitDeclarationOnly -p tsconfig.build.json",
        "build:js": "rollup -c",
        "build": "cross-env NODE_ENV=production npm run build:js && npm run build:types",
        "lint": "eslint --ext .ts ./src/",
        "lint:fix": "npm run lint -- --fix"
    },
    "devDependencies": {
        "@vue-layout/core": "^0.1.6",
        "cross-env": "^7.0.3",
        "vue": "^3.2.41"
    },
    "peerDependencies": {
        "@vue-layout/core": "^0.x",
        "vue": "^3.x"
    },
    "engines": {
        "node": ">=16"
    },
    "dependencies": {
        "regenerator-runtime": "^0.13.10",
        "smob": "^0.0.6"
    },
    "publishConfig": {
        "access": "public"
    }
}
