{
    "name": "@vue-layout/basic",
    "version": "0.4.0",
    "description": "A package containing basic components, to build for example multi level navigation menus.",
    "main": "dist/index.cjs.js",
    "browser": "dist/index.browser.js",
    "module": "dist/index.esm.js",
    "unpkg": "dist/index.iife.js",
    "types": "dist/types/index.d.ts",
    "files": [
        "dist/*"
    ],
    "keywords": [
        "vue",
        "layout",
        "basic",
        "components",
        "pagination",
        "navigation",
        "navbar",
        "tier",
        "multi-tier",
        "level",
        "vue-router",
        "typescript"
    ],
    "author": {
        "name": "Peter Placzek",
        "email": "contact@tada5hi.net",
        "url": "https://tada5hi.net"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/tada5hi/vue-layout.git",
        "directory": "packages/basic"
    },
    "scripts": {
        "prebuild": "rimraf ./dist",
        "build:types": "vue-tsc --declaration --emitDeclarationOnly -p tsconfig.build.json",
        "build:js": "rollup -c",
        "build": "cross-env NODE_ENV=production npm run build:js && npm run build:types",
        "lint": "eslint --ext .ts ./src/",
        "lint:fix": "npm run lint -- --fix",
        "test": "cross-env NODE_ENV=test jest --config ./test/jest.config.js",
        "test:coverage": "cross-env NODE_ENV=test jest --config ./test/jest.config.js --coverage"
    },
    "devDependencies": {
        "@types/jest": "^29.2.6",
        "@types/md5": "^2.3.2",
        "cross-env": "^7.0.3",
        "jest": "^29.4.2",
        "ts-jest": "^29.0.5",
        "vue": "^3.2.45",
        "vue-router": "^4.1.4",
        "vue-tsc": "^1.0.24"
    },
    "peerDependencies": {
        "vue": "^3.x",
        "vue-router": "^4.x"
    },
    "engines": {
        "node": ">=16"
    },
    "dependencies": {
        "@vue-layout/core": "^0.4.0",
        "@vue-layout/hyperscript": "^0.4.0",
        "md5": "^2.3.0"
    },
    "publishConfig": {
        "access": "public"
    }
}
