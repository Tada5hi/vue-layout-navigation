{
    "name": "@vue-layout/basic",
    "version": "0.2.1",
    "description": "A package containing basic components, to build for example multi level navigation menus.",
    "main": "dist/index.cjs.js",
    "browser": "dist/index.browser.js",
    "module": "dist/index.esm.js",
    "unpkg": "dist/index.iife.js",
    "types": "dist/types/index.d.ts",
    "files": [
        "dist/*"
    ],
    "keywords": [
        "vue",
        "layout",
        "basic",
        "components",
        "pagination",
        "navigation",
        "navbar",
        "tier",
        "multi-tier",
        "level",
        "vue-router",
        "typescript"
    ],
    "author": {
        "name": "Peter Placzek",
        "email": "contact@tada5hi.net",
        "url": "https://tada5hi.net"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/tada5hi/vue-layout.git",
        "directory": "packages/basic"
    },
    "scripts": {
        "prebuild": "rimraf ./dist",
        "build:types": "vue-tsc --declaration --emitDeclarationOnly -p tsconfig.build.json",
        "build:js": "rollup -c",
        "build": "cross-env NODE_ENV=production npm run build:js && npm run build:types",
        "lint": "eslint --ext .ts ./src/",
        "lint:fix": "npm run lint -- --fix",
        "test": "cross-env NODE_ENV=test jest --config ./test/jest.config.js",
        "test:coverage": "cross-env NODE_ENV=test jest --config ./test/jest.config.js --coverage"
    },
    "devDependencies": {
        "@babel/core": "^7.19.3",
        "@babel/preset-env": "^7.19.4",
        "@babel/preset-typescript": "^7.18.6",
        "@rollup/plugin-babel": "^6.0.0",
        "@rollup/plugin-commonjs": "^23.0.0",
        "@rollup/plugin-node-resolve": "^15.0.0",
        "@rollup/plugin-replace": "^5.0.0",
        "@types/jest": "^29.1.2",
        "@vue-layout/core": "^0.1.0",
        "@vue-layout/hyperscript": "^0.1.1",
        "cross-env": "^7.0.3",
        "jest": "^29.1.1",
        "rollup": "^3.1.0",
        "rollup-plugin-postcss": "^4.0.2",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-vue": "^6.0.0-beta.11",
        "ts-jest": "^29.0.3",
        "vue": "^3.2.40",
        "vue-router": "^4.1.4",
        "vue-tsc": "^1.0.7"
    },
    "peerDependencies": {
        "@vue-layout/core": "^0.x",
        "@vue-layout/hyperscript": "^0.x",
        "vue": "^3.x",
        "vue-router": "^4.x"
    },
    "engines": {
        "node": ">=16"
    },
    "dependencies": {
        "regenerator-runtime": "^0.13.9"
    },
    "publishConfig": {
        "access": "public"
    }
}
