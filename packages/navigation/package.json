{
    "name": "@vue-layout/navigation",
    "version": "5.0.1",
    "description": "A package for multi level navigations.",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        },
        "./core": {
            "types": "./core/index.d.ts",
            "import": "./core/index.mjs",
            "require": "./core/index.cjs"
        }
    },
    "main": "dist/index.cjs",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "files": [
        "core",
        "dist"
    ],
    "keywords": [
        "navigation",
        "navigation-elements",
        "vue",
        "layout",
        "components",
        "navigation",
        "navbar",
        "tier",
        "multi-tier",
        "level",
        "vue-router"
    ],
    "author": {
        "name": "Peter Placzek",
        "email": "contact@tada5hi.net",
        "url": "https://tada5hi.net"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/tada5hi/vue-layout.git",
        "directory": "packages/navigation"
    },
    "scripts": {
        "build:types": "vue-tsc --declaration --emitDeclarationOnly -p tsconfig.build.json",
        "build:js": "rollup -c",
        "build": "cross-env NODE_ENV=production rimraf dist && npm run build:js && npm run build:types",
        "test": "cross-env NODE_ENV=test jest --config ./test/jest.config.js",
        "test:coverage": "cross-env NODE_ENV=test jest --config ./test/jest.config.js --coverage"
    },
    "devDependencies": {
        "@vue-layout/core": "^5.0.1",
        "@vue-layout/link": "^5.0.1",
        "vue": "^3.3.10",
        "vue-router": "^4.2.2"
    },
    "peerDependencies": {
        "@vue-layout/core": "^5.0.1",
        "@vue-layout/link": "^5.0.1",
        "vue": "^3.x",
        "vue-router": "^4.x"
    },
    "engines": {
        "node": ">=16"
    },
    "publishConfig": {
        "access": "public"
    },
    "gitHead": "08baf626ef03e8db4a732502d795b7a21b3fd1ab"
}
